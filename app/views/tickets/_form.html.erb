<%= form_for @ticket do |f| %>
  <dl>
    <dt><%= f.label :name %></dt>
    <dd><%= f.text_field :name %></dd>
  </dl>
  <dl>
    <dt><%= f.label :body %></dt>
    <dd><%= f.text_field :body %></dd>
  </dl>
  <dl>
    <dt><%= f.label :status %></dt>
    <dd><%= f.text_field :status %></dd>
  </dl>
  <%= f.submit 'Update ticket' %>
<% end %>